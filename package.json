{
  "name": "typescript-bot",
  "version": "1.0.0",
  "description": "A bot built in typescript; Including features of Dependency Injection:InversifyJs, Discord API, Testing Libraries:Mocha,Chai, and ts-mockito",
  "main": "index.js",
  "scripts": {
    "start":"node src/index.js",
    "watch":"tsc -p tsconfig.json -w",
    "test": "mocha -r ts-node/register \"tests/**/*.spec.ts\""
  },
  "keywords": [
    "DI",
    "Dependency",
    "Injection",
    "Testing",
    "Typescript",
    "Bots",
    "Discord"
  ],
  "author": "Aaron Ogenrwot",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^14.0.4",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "inversify": "^5.0.1",
    "reflect-metadata": "^0.1.13",
    "typescript": "^3.9.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "mocha": "^7.1.2",
    "ts-mockito": "^2.5.0",
    "ts-node": "^8.10.1"
  }
}
